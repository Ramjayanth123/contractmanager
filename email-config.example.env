# Email Service Configuration
# Copy these variables to your .env file

# Set to 'true' to enable the email notification service
VITE_EMAIL_SERVICE_ENABLED=false

# Gmail address to use for sending notifications
VITE_EMAIL_ADDRESS=your-email@gmail.com

# Google App Password (not your regular Gmail password)
# Generate at: https://myaccount.google.com/apppasswords
VITE_EMAIL_APP_PASSWORD=your-google-app-password

# How often to check for expiring contracts (in hours)
VITE_EMAIL_CHECK_INTERVAL_HOURS=24 